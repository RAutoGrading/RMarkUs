#'
#' Helper function to run the distribution function using the string name
#'
#' @param fct Function to run
#' @param ... Parameters for function
#' @return List
#' @examples
#'
#' @export
# run_function
run_function <- function(fct, ...) {
get(fct)(...)
}
run_function(rnorm, 0, 1)
run_function('rnorm', 10, 0, 1)
run_function('rbinom', 10, 2, 3)
rbinom
run_function('rbinom', 10, 2, 0.1)
run_function('rbinom', 10, 2, 0.5)
generateData <- function(distribution, n, seedNum, ...){
set.seed(seedNum)
distr_fnc <-  paste('r', distribution, sep='')
randomData <- run_function(distr_fnc, n, ...)
return(as.data.frame(randomData))
}
generate_data(rnorm, 10, 2)
generateData(rnorm, 10, 2, 0, 1)
